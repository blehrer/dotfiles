if [[ "Darwin" = $(uname) ]]; then
    if has mise ; then
        mise activate $(echo $0 | xargs basename)
    fi
    if has starship ; then
        export STARSHIP_CONFIG=${XDG_CONFIG_HOME:-$HOME/.config}/starship/starship.toml
        eval "$(starship init $shellname)"
    fi
fi

