[core]
    autocrlf = input
	excludesfile = /Users/a206672215/.config/dotfiles/git/gitignore_global
[log]
    follow = true
[advice]
    skippedCherryPicks=false
[alias]
    amend = commit -a --amend --no-edit
    branch-name = rev-parse --abbrev-ref HEAD
    backup = "!git push -f -u origin HEAD:bl-backup/$(git branch-name)"
    blaame = blame -w -C -C -C
    clone-gh = "!git clone git@github.com:$1"
    changelist = "!upstream=$(git rev-parse --abbrev-ref --symbolic-full-name @{u}) && \
        echo Changelist, relative to upstream branch $upstream && \
        git log --oneline --graph $upstream..@"
    delete-from-origin = "!git push -d origin $(git branch-name | tr -d '[:space:]')"
    done = "!git branch -m ${1:-`git branch-name`} 'DONE/'${1:-`git branch-name`}"
    fixup = commit -a --fixup
    updiff = diff "@{u}"
[init]
    defaultBranch = main
[diff]
    #colorMoved = dimmed-zebra
    colorMoved = blocks
    #colorMoved = no
    #color-moved-ws = allow-indentation-change
    tool = vimdiff
    noprefix = true
    renameLimit = 50000
[diff "zip"]
    textconv = unzip -v
    binary = true
[merge]
    tool = vimdiff
[fetch]
    prune = true
[prune]
    prune = true
[rebase]
    autosquash = true
[status]
    submoduleSummary = false
[pull]
    rebase = false
    prune = true
[maintenance]
	repo = /Users/a206672215/workplace/github.com/NBCUniversalOTS/tll-recon-api
[rerere]
	enabled = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Users/a206672215/Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[user]
	name = Lehrer, Brian (Contractor-NBCUniversal)
	email = Brian.Lehrer@nbcuni.com
