## Environment Variables
### Places
export WORKPLACE=~/workplace
export CONFIG_DIR=~/.config
export DOTFILES_HOME=$CONFIG_DIR/dotfiles
export PYENV_ROOT="$HOME/.pyenv"
export NVM_DIR="$HOME/.nvm"
export SDKMAN_DIR="/usr/local/sdkman"
export DISPLAY=:0
export ECLIPSE_HOME="$HOME/eclipse/java-2019-12/eclipse"
export WIN_HOME="/mnt/c/Users/Brian"
export EDITOR="/usr/bin/vim"
export CP_CONFIG_REPO=$WORKPLACE/cp-config-repo
export PAGER="/usr/bin/less -i"

### Parameters
export HISTSIZE=1000000

